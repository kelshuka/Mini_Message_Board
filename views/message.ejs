<!DOCTYPE html>
<html lang="en">
    <head>
        <link rel="stylesheet" href="./styles.css">
    </head>
<body>
    <section class="allMessage">
        
        <% for (let i=0; i < messages.length; i++){ %>
            <section class="oneMessage">
                <section>
                    User: <span> <%= messages[i].user %> </span>
                </section>

                <section>
                    Message: <span> <%= messages[i].text %> </span>
                </section>

                <section>
                    Date added: <span> <%= messages[i].added %> </span>
                </section>
            </section>
        <% } %>
        
    </section>
</body>
</html>
